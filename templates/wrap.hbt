<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>{{config.title}}</title>
        {{#if config.css}}
            {{#each config.css}}
                <link rel="stylesheet" href="{{this}}">
            {{/each}}
        {{/if}}
        <link rel="stylesheet" href="styles/main.css">
    </head>
    <body>

        <div class="reveal">
            <div class="slides">
            {{#each collections.slides}}
                {{{ contents }}}
            {{/each}}
            </div>
        </div>

        <a class="skip-to-index" href="#/83">Skip to Index</a>

        <script src="scripts/reveal.min.js"></script>
        <script>
        Reveal.initialize({{{json config.reveal}}});
        </script>
    </body>
</html>
